<?xml version='1.0' encoding='utf-8'?>
<widget id="io.nimhans.wellness" version="1.1.2" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Wellness-check</name>
    <description>
        Wellness- check is an internet- based program to track various aspects.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
       Wellness-check Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
</widget>
